{
  "name": "js-index-builder",
  "version": "0.0.1",
  "description": "Cache buster for mustache",
  "main": "index.js",
  "scripts": {
    "init": "curl --disable --silent --show-error --location --max-time 30 'https://raw.githubusercontent.com/ViBiOh/scripts/main/bootstrap' | bash -s 'git_hooks'",
    "format": "prettier --write \"./*.js\"",
    "style": "eslint --fix \"./*.js\"",
    "build": "npm run format && npm run style"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-index-builder.git"
  },
  "keywords": [
    "mutache",
    "cache-buster"
  ],
  "author": "Vincent Boutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-index-builder/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-index-builder",
  "devDependencies": {
    "clean-css": "5.1.4",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "prettier": "2.3.2"
  },
  "dependencies": {
    "glob": "7.1.7",
    "js-utils": "github:ViBiOh/js-utils#main",
    "mkdirp": "1.0.4",
    "mustache": "4.2.0",
    "yargs": "17.0.1"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
