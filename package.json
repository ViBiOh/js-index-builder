{
  "name": "js-index-builder",
  "version": "0.0.1",
  "description": "Cache buster for mustache",
  "main": "index.js",
  "scripts": {
    "init": "curl --disable --silent --show-error --location --max-time 30 'https://raw.githubusercontent.com/ViBiOh/scripts/main/bootstrap.sh' | bash -s 'git_hooks'",
    "format": "prettier --write \"./*.mjs\"",
    "style": "eslint --fix \"./*.mjs\"",
    "build": "npm run format && npm run style"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-index-builder.git"
  },
  "keywords": [
    "mutache",
    "cache-buster"
  ],
  "author": "Vincent Boutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-index-builder/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-index-builder",
  "devDependencies": {
    "clean-css": "5.3.3",
    "eslint": "10.0.2",
    "prettier": "3.8.1"
  },
  "dependencies": {
    "glob": "13.0.6",
    "mkdirp": "3.0.1",
    "mustache": "4.2.0",
    "yargs": "18.0.0"
  }
}
