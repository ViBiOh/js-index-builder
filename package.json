{
  "name": "js-index-builder",
  "version": "0.0.1",
  "description": "Cache buster for mustache",
  "main": "index.js",
  "scripts": {
    "init": "curl --disable --silent --show-error --location --max-time 30 'https://raw.githubusercontent.com/ViBiOh/scripts/main/bootstrap' | bash -s 'git_hooks'",
    "format": "prettier --write \"./*.js\"",
    "style": "eslint --fix \"./*.js\"",
    "build": "npm run format && npm run style"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-index-builder.git"
  },
  "keywords": [
    "mutache",
    "cache-buster"
  ],
  "author": "Vincent Boutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-index-builder/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-index-builder",
  "devDependencies": {
    "clean-css": "5.3.2",
    "eslint": "8.44.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "prettier": "3.0.0"
  },
  "dependencies": {
    "glob": "10.3.1",
    "mkdirp": "3.0.1",
    "mustache": "4.2.0",
    "yargs": "17.7.2"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
