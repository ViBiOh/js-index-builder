{
  "name": "js-index-builder",
  "version": "0.0.1",
  "description": "Cache buster for mustache",
  "main": "index.js",
  "scripts": {
    "prettier": "./node_modules/.bin/prettier --print-width 100 --single-quote --trailing-comma all --color --write \"./*.js\"",
    "eslint": "./node_modules/.bin/eslint --fix \"./*.js\"",
    "codestyle": "npm run prettier && npm run eslint",
    "build": "npm run codestyle"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-index-builder.git"
  },
  "keywords": [
    "mutache",
    "cache-buster"
  ],
  "author": "Vincent Boutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-index-builder/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-index-builder",
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "latest",
    "prettier": "latest"
  },
  "dependencies": {
    "clean-css": "latest",
    "glob": "latest",
    "jpegtran-bin": "latest",
    "js-utils": "ViBiOh/js-utils",
    "mkdirp": "latest",
    "mustache": "latest",
    "yargs": "latest"
  }
}
