{
  "name": "js-index-builder",
  "version": "0.0.1",
  "description": "Cache buster for mustache",
  "main": "index.js",
  "scripts": {
    "postinstall": "chmod +x *.js",
    "format": "prettier --write \"./*.js\"",
    "lint": "eslint --fix \"./*.js\"",
    "build": "npm run format && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-index-builder.git"
  },
  "keywords": [
    "mutache",
    "cache-buster"
  ],
  "author": "Vincent Boutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-index-builder/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-index-builder",
  "devDependencies": {
    "clean-css": "4.2.1",
    "eslint": "5.12.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.15.0",
    "prettier": "1.16.1"
  },
  "dependencies": {
    "glob": "7.1.3",
    "js-utils": "github:ViBiOh/js-utils",
    "mkdirp": "0.5.1",
    "mustache": "3.0.1",
    "yargs": "12.0.5"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
