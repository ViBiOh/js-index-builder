{
  "name": "js-index-builder",
  "version": "0.0.1",
  "description": "Cache buster for mustache",
  "main": "index.js",
  "scripts": {
    "postinstall": "chmod +x *.js",
    "format": "prettier --write \"./*.js\"",
    "lint": "eslint --fix \"./*.js\"",
    "build": "npm run format && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-index-builder.git"
  },
  "keywords": [
    "mutache",
    "cache-buster"
  ],
  "author": "Vincent Boutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-index-builder/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-index-builder",
  "devDependencies": {
    "eslint": "4",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "latest",
    "prettier": "latest"
  },
  "dependencies": {
    "clean-css": "latest",
    "glob": "latest",
    "js-utils": "ViBiOh/js-utils",
    "mkdirp": "latest",
    "mustache": "latest",
    "yargs": "latest"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
